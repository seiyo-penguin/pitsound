




    <h3>
      Congratulations! You are the first to upload this song!
      アレンジのオリジナル曲を登録してください。
    </h3>


  <form id="search" onsubmit="return false;">
    <input type="text" class="textbox" value=<%= @songtitle%> id="q" size="40" name="songtitle">
    <button type="submit" value="この曲の動画を検索" class="btn"><span></span></button>
  </form>


    <%= form_for @music, :url => {:action => 'create'} do |f| %>
      <%= f.label :songtitle, "曲名"%>
      <!-- <input type="text" class="textbox" value=<%= @songtitle%> id="q" size="40" name="songtitle"> -->
      <%= f.hidden_field :songtitle, value: @songtitle %><br>
      <%= f.label :uid, "以下の動画から選ぶまたはYoutubeの埋め込みコードを貼り付ける" %>
      <%= f.text_field :uid %>
      <%= f.submit "この曲のアレンジを投稿する" %>
      <div class="insert_youtube">
      </div>

<% end %>